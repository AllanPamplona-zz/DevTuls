<!DOCTYPE html>
<html>
    <head>
        <meta charset='UTF-8'>
        <title>DevTuls</title>
        <!--Materialize-->
        <link rel="stylesheet" href="/pub/stylesheets/materialize.min.css">
        <!-- Aquí se ve como usar css desde la carpeta public -->
        <link rel="stylesheet" href="/pub/stylesheets/style.css">
        <link rel="stylesheet" href="/pub/select2/dist/css/select2.min.css">
        <link href="https://fonts.googleapis.com/icon?family=Material+Icons" rel="stylesheet"
    </head>
    <body>
  <!-- Cabecera-->
  <header  class="blue lighten-3 white-text center-align z-depth-3">
    <!--Nav-->
    <div class="navbar-fixed">
      <nav>
          <div class="nav-wrapper blue lighten-3 white-text">
              <a href="#" class="brand-logo center">DevTuls</a>
              <ul id="nav-mobile" class="left hide-on-med-and-down">
                      <!--<li><a href="#/">Home</a></li>-->
                      <li><a href="/logout">Logout</a></li>

            </ul>
        </div>
      </nav>
   </div>
 </header>

   <!--Tablero-->
   <main>
     <!--Modal crear-->
     <div id="modal1" class="modal modal-fixed-footer">
       <div class="modal-content">
         <label for="nombre">Nombre del proyecto</label>
         <input placeholder="Nombre del proyecto" id="nombre" type="text" class="validate">
         <label for="nombre">Agregar miembro</label>
         <input placeholder="email" id="email" type="text" class="validate">
         <ul id="coleccion" class="collection">
         </ul>
       </div>
       <div class="modal-footer">
         <a href="#!" id="añadir" class="modal-action modal-action waves-effect waves-green btn-flat">Aceptar</a>
          <a href="#!" id="close"  class="modal-action modal-close waves-effect waves-green btn-flat">Cancelar</a>
          <a href="#!" id="agregar" class="modal-action waves-effect waves-green btn-flat">Agregar usuario</a>
       </div>
     </div>
      </div>
     <!--Fin modal crear-->
       <div class="caja">         
         <select  class="clase" id="listaselector">
          <option value="disable" disabled selected>Choose your option</option>
          {{#each proy}}
          <option value={{this._id}}> {{this.nombre}}</option>
          {{/each}}
        </select>
        <a id="selectorid" class="waves-effect blue lighten-3 z-depth-4 btn boton-selector">button</a>
        <a id="crearid" href="#modal1" class="waves-effect blue lighten-3 z-depth-4 btn boton-selector">Crear</a>
    </div>
   </main>
   <!--Footer-->
   <footer class="page-footer blue lighten-3 z-depth-4">
     <div class="white-text center-align">
       Desarrollado por:
       <br>
       ACJM
     </div>
   </footer>
<!--Importar Scripts-->
<script src='/node/jquery.min.js'></script>
<script src='/node1/jquery-ui.min.js'></script>
<script src='/socket/socket.io.js'></script>
<script src='/pub/js/selector.js'></script>
<script src='/pub/select2/dist/js/select2.min.js'></script>
<script src='/pub/js/materialize.min.js'></script>           
<script>
    $(document).ready(function(){
        $(".clase").select2({
            minimumResultsForSearch: 10
        });
    });
</script>
 
    </body>
</html>
